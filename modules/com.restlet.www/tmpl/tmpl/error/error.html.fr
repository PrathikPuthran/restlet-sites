<#include "/header.ftl"/>

<#global javascript></#global>
<#global content>
<@h3 "Description" "error">
<div>
<#noparse>
<#if status.description??>
	${status.description}
<#else>
    Aucune description n'est disponible pour cette status d'erreur.
</#if>
</#noparse>
</div>
